issn = {1081-0706},
journal = {Annual Review of Cell and Developmental Biology},
keywords = {2,ca 2,channels mediate ca 2,contraction,cur-,cyclic amp,electrophysiological studies reveal different,entry into cells in,exocytosis,g proteins,protein phosphorylation,response,s abstract voltage-gated ca,snare proteins,synaptic transmission,to membrane depolarization},
month = {nov},
