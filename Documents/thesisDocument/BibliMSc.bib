journal = {Biotechnology and bioengineering},
keywords = {Complex Mixtures,Complex Mixtures: analysis,Complex Mixtures: chemistry,Computer Simulation,Fractional Precipitation,Models, Chemical,Molecular Weight,Phase Transition,Proteins,Proteins: analysis,Proteins: chemistry,Static Electricity,Statistics as Topic,Water,Water: analysis,Water: chemistry},
month = {may},
