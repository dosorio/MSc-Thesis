keywords = {Antimicrobial peptides,Nuclear magnetic resonance,Oriented lipid bilayers,antimicrobial peptides,nuclear magnetic resonance,oriented lipid bilayers},
month = {sep},
number = {9},
pages = {2173--9},
pmid = {24508758},
publisher = {Elsevier B.V.},
title = {{Investigation of the mechanism of action of novel amphipathic peptides: insights from solid-state NMR studies of oriented lipid bilayers.}},
