journal = {The Journal of biological chemistry},
keywords = {Amino Acid Sequence,Animals,Anti-Infective Agents,Anti-Infective Agents: isolation {\&} purification,Blotting, Northern,Chromatography, High Pressure Liquid,Electrophoresis,Molecular Sequence Data,Peptides,Peptides: pharmacology,RNA,RNA: analysis,Skin,Skin: chemistry,Stomach,Stomach: chemistry,Xenopus laevis},
month = {oct},
