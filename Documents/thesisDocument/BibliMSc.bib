file = {:Users/daniel/Documents/Mendeley/Nascimento et al. - 2014 - Elucidation of mechanisms of interaction of a multifunctional peptide Pa-MAP with lipid membranes.pdf:pdf},
issn = {0006-3002},
journal = {Biochimica et biophysica acta},
keywords = {Antimicrobial,Electrical impedance spectroscopy,Membrane,Multifunctional peptide,Pleuronectes americanus,multifunctional peptide,pleuronectes americanus},
month = {aug},
