journal = {BMC systems biology},
keywords = {Adipocytes,Adipocytes: metabolism,Alanine,Alanine: metabolism,Computer Simulation,Diabetes Mellitus,Diabetes Mellitus: metabolism,Genome, Human,Hepatocytes,Hepatocytes: metabolism,Humans,Metabolic Networks and Pathways,Metabolomics,Models, Biological,Muscle Cells,Muscle Cells: metabolism,Obesity,Obesity: metabolism,Systems Biology},
