issn = {1520-5126},
journal = {Journal of the American Chemical Society},
keywords = {Cholesterol,Cholesterol: chemistry,Membranes, Artificial,Models, Molecular,Molecular Dynamics Simulation,Phospholipids,Phospholipids: chemistry,Solubility,Water,Water: chemistry},
month = {mar},
number = {11},
pages = {5351--61},
pmid = {22372465},
title = {{Large influence of cholesterol on solute partitioning into lipid membranes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22372465},
volume = {134},
year = {2012}
}
