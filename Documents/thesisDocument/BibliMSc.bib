journal = {Biochimica et biophysica acta},
keywords = {Antimicrobial Cationic Peptides,Antimicrobial Cationic Peptides: metabolism,Cell Membrane,Cell Membrane: chemistry,Cell Membrane: metabolism,Dimyristoylphosphatidylcholine,Dimyristoylphosphatidylcholine: chemistry,Dimyristoylphosphatidylcholine: metabolism,Membrane Lipids,Molecular Sequence Data,Molecular Structure,Protein Structure, Secondary},
month = {feb},
number = {2},
pages = {194--204},
pmid = {22100601},
publisher = {Elsevier B.V.},
title = {{Mechanism of structural transformations induced by antimicrobial peptides in lipid membranes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22100601},
volume = {1818},
year = {2012}
}
@article{Hancock2000,
abstract = {Cationic antimicrobial peptides are found in all living species. A single animal can contain {\textgreater}24 different antimicrobial peptides, which fall into four structural classes. These peptides are produced in large quantities at sites of infection and/or inflammation and can have broad-spectrum antibacterial, antifungal, antiviral, antiprotozoan and antisepsis properties. In addition, they interact directly with host cells to modulate the inflammatory process and innate defences.},
author = {Hancock, R E and Diamond, G},
