journal = {Nature protocols},
keywords = {Cells,Cells: metabolism,Computational Biology,Computational Biology: methods,Computer Simulation,Metabolic Networks and Pathways,Metabolic Networks and Pathways: physiology,Models, Biological,Software,Systems Biology,Systems Biology: methods},
number = {3},
pages = {727--38},
pmid = {17406635},
title = {{Quantitative prediction of cellular metabolism with constraint-based models: the COBRA Toolbox.}},
