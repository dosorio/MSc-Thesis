file = {:Users/daniel/Documents/Mendeley/Laveti et al. - 2013 - Anti-inflammatory treatments for chronic diseases a review.pdf:pdf},
isbn = {1871-5281},
issn = {2212-4055},
journal = {Inflammation {\&} allergy drug targets},
keywords = {asthma,cancer,cardiovascular disease,cns disorders,diabetes,inflammation,inflammatory markers,obesity},
number = {5},
pages = {349--61},
pmid = {23876224},
title = {{Anti-inflammatory treatments for chronic diseases: a review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23876224},
volume = {12},
year = {2013}
}
