journal = {Endoplasmic Reticulum Stress in Diseases},
keywords = {circadian,communicable diseases,inflammation,metabolism,oscillators,systems biology,upr},
number = {1},
pages = {30--52},
title = {{The unfolded protein response, inflammation, oscillators, and disease: a systems biology approach}},
url = {http://www.degruyter.com/view/j/ersc.2015.2.issue-1/ersc-2015-0003/ersc-2015-0003.xml},
volume = {2},
year = {2015}
}
