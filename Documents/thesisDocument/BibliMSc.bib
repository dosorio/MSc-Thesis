pages = {10677--91},
pmid = {19586028},
title = {{Protein hydration dynamics and molecular mechanism of coupled water-protein fluctuations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19586028},
volume = {131},
year = {2009}
}
