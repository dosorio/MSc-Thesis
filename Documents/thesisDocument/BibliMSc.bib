journal = {British Journal of Nutrition},
keywords = {1,amino acids,hypertension and cvd,including type 2 diabetes,increased,inflammation,levels of inflammatory and,main,number of chronic diseases,obesity,obesity is a major,oxidative biomarkers are the,oxidative stress,risk factor for a},
number = {01},
pages = {57--61},
pmid = {21996294},
title = {{Histidine and arginine are associated with inflammation and oxidative stress in obese women}},
