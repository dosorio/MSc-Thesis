pages = {2280--8},
pmid = {24747526},
publisher = {Elsevier B.V.},
title = {{How reliable are molecular dynamics simulations of membrane active antimicrobial peptides?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24747526},
volume = {1838},
year = {2014}
}
