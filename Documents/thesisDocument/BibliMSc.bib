issn = {0006-3002},
journal = {Biochimica et biophysica acta},
keywords = {Anti-Bacterial Agents,Anti-Bacterial Agents: chemistry,Anti-Infective Agents, Local,Anti-Infective Agents, Local: chemistry,Bacteria,Bacteria: chemistry,Calorimetry, Differential Scanning,Cell Membrane,Cell Membrane: chemistry,Gramicidin,Gramicidin: chemistry,Intracellular Membranes,Intracellular Membranes: chemistry,Lipid Bilayers,Lipid Bilayers: chemistry,Magnetic Resonance Spectroscopy,Models, Molecular,Permeability,Phospholipids,Phospholipids: chemistry,Protein Conformation,Spectrometry, Fluorescence,Spectroscopy, Fourier Transform Infrared,Structure-Activity Relationship,Thermodynamics},
month = {dec},
