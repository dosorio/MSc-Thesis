@article{Williams2012,
abstract = {Iron, an essential element used for a multitude of biochemical reactions, abnormally accumulates in the CNS of patients with multiple sclerosis (MS). The mechanisms of abnormal iron deposition in MS are not fully understood, nor do we know whether these deposits have adverse consequences, that is, contribute to pathogenesis. With some exceptions, excess levels of iron are represented concomitantly in multiple deep gray matter structures often with bilateral representation, whereas in white matter, pathological iron deposits are usually located at sites of inflammation that are associated with veins. These distinct spatial patterns suggest disparate mechanisms of iron accumulation between these regions. Iron has been postulated to promote disease activity in MS by various means: (i) iron can amplify the activated state of microglia resulting in the increased production of proinflammatory mediators; (ii) excess intracellular iron deposits could promote mitochondria dysfunction; and (iii) improperly managed iron could catalyze the production of damaging reactive oxygen species (ROS). The pathological consequences of abnormal iron deposits may be dependent on the affected brain region and/or accumulation process. Here, we review putative mechanisms of enhanced iron uptake in MS and address the likely roles of iron in the pathogenesis of this disease.},
author = {Williams, Rachel and Buchheit, Cassandra L. and Berman, Nancy E J and Levine, Steven M.},
doi = {10.1111/j.1471-4159.2011.07536.x},
file = {:Users/daniel/Documents/Mendeley/Williams et al. - 2012 - Pathogenic implications of iron accumulation in multiple sclerosis.pdf:pdf},
isbn = {1471-4159 (Electronic)$\backslash$r0022-3042 (Linking)},
issn = {00223042},
journal = {Journal of Neurochemistry},
keywords = {DMT1,experimental autoimmune encephalomyelitis,ferritin,neurodegeneration,reactive oxygen species,transferrin receptor},
number = {1},
pages = {7--25},
pmid = {22004421},
title = {{Pathogenic implications of iron accumulation in multiple sclerosis}},
volume = {120},
year = {2012}
}
@article{Rael2004,
abstract = {Although N-acetyl-L-aspartate (NAA) has been shown to be important to myelin synthesis and osmotic regulation, the biological rationale for the high levels of NAA found in the brain remains unknown. Here, a human astroglial cell line (STTG) was treated with NAA and stimulated with ionomycin, ionomycin/PMA, or IL-1??. PGE2 levels in ionomycin-stimulated STTG cells decreased by 76{\%} and {\textgreater}95{\%} at NAA concentrations of 10 and 20mM, respectively. NAA also decreased the levels of COX-2 protein and activated NF-??B in IL-1??-stimulated STTG cells but had little effect on unstimulated cells. Also, NAA significantly decreased intracellular calcium levels in ionomycin/PMA-stimulated cells. NAA had no effect on total COX-2 activity or COX-2 mRNA. Acetylation of I??B?? kinase, an acetylation target of aspirin, was not observed when NAA was present. These results demonstrate that NAA appears to be important in the modulation of inflammation in the human STTG astroglial cell line. The results of these findings are discussed in relation to neuronal pathologies that exhibit abnormal NAA levels within the brain. ?? 2004 Elsevier Inc. All rights reserved.},
