file = {:Users/daniel/Documents/Mendeley/Chen, Mant, Hodges - 2002 - Determination of stereochemistry stability coefficients of amino acid side-chains in an amphipathic alpha-he.pdf:pdf},
issn = {1397-002X},
journal = {The journal of peptide research : official journal of the American Peptide Society},
keywords = {Amino Acid Sequence,Amino Acid Substitution,Amino Acids,Amino Acids: chemistry,Chromatography, High Pressure Liquid,Circular Dichroism,Hydrogen Bonding,Molecular Conformation,Molecular Sequence Data,Peptides,Peptides: chemistry,Protein Denaturation,Protein Structure, Secondary,Temperature},
month = {jan},
