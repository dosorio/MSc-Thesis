isbn = {2050-6511},
issn = {2050-6511},
journal = {BMC pharmacology {\&} toxicology},
keywords = {Animals,Astrocytes,Astrocytes: metabolism,Glutamic Acid,Glutamic Acid: metabolism,Glutamine,Glutamine: metabolism,Humans,Manganese,Manganese: toxicity,Neurotoxicity Syndromes,Neurotoxicity Syndromes: etiology,Neurotoxicity Syndromes: metabolism},
pages = {23},
pmid = {23594835},
title = {{Role of astrocytes in manganese mediated neurotoxicity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3637816{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {14},
year = {2013}
}
@article{Patil2005,
abstract = {Epidemiological studies suggest that high fat diets significantly increase the risk of Alzheimer's disease (AD). In addition, the AD brain is characterized by high fatty acid content compared to that of healthy subjects. Nevertheless, the basic mechanism relating elevated fatty acids and the pathogenesis of AD remains unclear. The present study examines the role of fatty acids in causing hyperphosphorylation of the tau protein, one of the characteristic signatures of AD pathology. Hyperphosphorylation of tau disrupts the cell cytoskeleton and leads to neuronal degeneration. Here, primary rat cortical neurons and astrocytes were treated with saturated free fatty acids (FFAs), palmitic and stearic acids. There was no change in the levels of phosphorylated tau in rat cortical neurons treated directly with these FFAs. The conditioned media from FFA-treated astrocytes, however, caused hyperphosphorylation of tau in the cortical neurons at AD-specific phospho-epitopes. Co-treatment of neurons with N-acetyl cysteine, an antioxidant, reduced FFA-induced hyperphosphorylation of tau. The present results establish a central role of FFAs in causing hyperphosphorylation of tau through astroglia-mediated oxidative stress. {\textcopyright} 2005 Elsevier Ireland Ltd. All rights reserved.},
author = {Patil, Sachin and Chan, Christina},
doi = {10.1016/j.neulet.2005.05.003},
file = {:Users/daniel/Documents/Mendeley/Patil, Chan - 2005 - Palmitic and stearic fatty acids induce Alzheimer-like hyperphosphorylation of tau in primary rat cortical neurons.pdf:pdf},
issn = {03043940},
journal = {Neuroscience Letters},
keywords = {Alzheimer's disease,Oxidative stress,Primary cortical neurons,Saturated fatty acids,Tau hyperphosphorylation},
number = {3},
pages = {288--293},
pmid = {15939536},
title = {{Palmitic and stearic fatty acids induce Alzheimer-like hyperphosphorylation of tau in primary rat cortical neurons}},
volume = {384},
year = {2005}
}
@article{Molofsk2012,
abstract = {Astrocytes are no longer seen as a homogenous population of cells. In fact, recent studies indicate that astrocytes are morphologically and functionally diverse and play critical roles in neurodevelopmental diseases such as Rett syndrome and fragile X mental retardation. This review summarizes recent advances in astrocyte development, including the role of neural tube patterning in specification and developmental functions of astrocytes during synaptogenesis. We propose here that a precise understanding of astrocyte development is critical to defining heterogeneity and could lead advances in understanding and treating a variety of neuropsychiatric diseases.},
author = {Molofsk, Anna V. and Krenick, Robert and Ullian, Erik and Tsai, Hui Hsin and Deneen, Benjamin and Richardson, William D. and Barres, Ben. and Rowitch, David H.},
doi = {10.1101 /gad.188326.112},
