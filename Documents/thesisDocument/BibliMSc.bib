doi = {10.1093/bioinformatics/btu040},
file = {:Users/daniel/Documents/Mendeley/Kessler, Taylor, Dunson - 2014 - Learning phenotype densities conditional on many interacting predictors.pdf:pdf},
issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
