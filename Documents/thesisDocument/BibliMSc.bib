isbn = {1471-4159},
issn = {00223042},
journal = {Journal of Neurochemistry},
keywords = {Alzheimer's disease,Brain aging,Differential proteomics,Neuron and astrocyte cultures,Senescence-accelerated prone mouse strain 8},
