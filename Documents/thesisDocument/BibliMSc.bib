journal = {Molecular systems biology},
keywords = {Algorithms,Computational Biology,Humans,Liver,Liver/anatomy {\&} histology/metabolism,Liver: anatomy {\&} histology,Liver: metabolism,Models,Theoretical},
number = {401},
pages = {401},
pmid = {20823844},
publisher = {Nature Publishing Group},
title = {{Computational reconstruction of tissue-specific metabolic models: application to human liver metabolism}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2964116{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
year = {2010}
}
@article{Kuepfer2014,
abstract = {Metabolic flux analysis (MFA) was performed on mouse embryonic stem cells cultured under three distinct differentiation conditions: classical embryoid body formation (EB), and on surfaces coated with either gelatin (GEL) or matrigel (MAT). MFA was based on 15 metabolic reactions and eight transport steps, and was carried out based on measurements of four substrates and/or metabolites: glucose, lactate, glutamine, and glutamate. Fluxes representing biomass production remained fairly constant for all three culture conditions with at most a 40{\%} variation. In contrast, major temporal variations, up to 500{\%}, were observed for all other major central metabolic fluxes across all culture conditions. Fluxes were compared to gene-expression patterns measured by microarray analysis. Particularly interesting is the correlation between the metabolic fluxes with expression patterns of the corresponding genes of the pyruvate to lactate reaction, whereby the genes for several isoforms of the lactate dehydrogenase enzyme were examined. The patterns of this flux were notably different in the EB cultures compared to the GEL and MAT cultures and reflected differences in oxygen and nutrient transport in EB vs. the GEL and MAT cultures. Another novel finding of this study is an event occurring between Days 4 and 5 of differentiation, which was identified by a notable change in both the metabolic fluxes and gene-expression patterns. This suggests that metabolic patterns can be used as effective beacons of changes in differentiating stem cells. Overall, and qualitatively, core metabolic fluxes, under the three culture conditions examined, correlated well with gene-expression patterns.},
author = {Kuepfer, Lars},
doi = {10.1007/978-1-4939-1170-7_1},
file = {:Users/daniel/Documents/Mendeley/Kuepfer - 2014 - Stoichiometric modelling of microbial metabolism.pdf:pdf},
isbn = {1389-1723},
issn = {10643745},
journal = {Methods in Molecular Biology},
keywords = {Data integration,FBA,Flux distribution,Fluxes,Metabolic engineering,Metabolic network,Objective function,Stoichiometric model},
number = {1},
pages = {3--18},
pmid = {20945480},
title = {{Stoichiometric modelling of microbial metabolism}},
volume = {1191},
year = {2014}
}
