journal = {Journal of molecular graphics {\&} modelling},
keywords = {Bacillus subtilis,Bacillus subtilis: enzymology,Basidiomycota,Basidiomycota: enzymology,Benzothiazoles,Benzothiazoles: chemistry,Biodegradation, Environmental,Computational Biology,Computational Biology: methods,Crystallography, X-Ray,Laccase,Laccase: metabolism,Ligands,Software,Sulfonic Acids,Sulfonic Acids: chemistry},
month = {jan},
number = {5},
pages = {845--9},
pmid = {17606396},
title = {{An in silico [correction of insilico] approach to bioremediation: laccase as a case study.}},
