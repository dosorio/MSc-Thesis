isbn = {0270-6474 (Print)$\backslash$r0270-6474 (Linking)},
issn = {0270-6474},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {brain,cells,channels,described in cerebellar granule,hypomyelination,injury,ischemia,l-type ca 2 ϩ,nels that have been,rat cns,reactive astrocytes,resemble q-type ca 2,their functional properties closely,zhang,ϩ chan-},
number = {7},
